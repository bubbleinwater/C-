練習問題 2.

#include "../../std_lib_facilities.h"

int main()
{
	vector <double> temps;

	for (double temp; cin >> temp;)
		temps.push_back(temp);

	//温度の平均値
	double sum = 0;
	for (double x : temps)sum += x;
	cout << "Average temperature: " << sum / temps.size() << '\n';

	//温度の中央値
	sort(temps);
	if (temps.size() % 2 == 0) {
		cout << "Median temperature: " << (temps[temps.size() / 2 - 1] + temps[temps.size() / 2]) / 2 << '\n';
	}
	else
		cout << "Median temperature: "<<temps[temps.size() / 2] << '\n';

	return 0;
}

練習問題 3.

#include "../../std_lib_facilities.h"

int main()
{
	vector <double> roads;
	double sum =0;

	cout << "隣の都市まで行くのに通る道の長さを入力してください。\n";

	for (double road; cin >> road;)
		roads.push_back(road);
	sort(roads);

	for (double x : roads)
		sum += x;

	cout << "道順での最も短い道は" << roads[0] << '\n';
	cout << "道順での最長の道は" << roads[roads.size()-1] << "です\n";
	cout << "それぞれの平均は" << sum / roads.size() << "です\n";

	return 0;
}


