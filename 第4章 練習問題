			cout << "今のマスのグレイン量は1です。\n";
			premasu += 1;
			sum = 1;
		}
		else {
			cout << "\n現在のマス目は" << i << "です。\n";
			cout << "今のマスのグレイン量は" << premasu * 2 << "です\n";
			premasu = premasu * 2;
			sum += premasu;
