練習問題 2.
#include "std_lib_facilities.h"

int main()
{
	vector<double> temps;
	for (double temp; cin >> temp;)
		temps.push_back(temp);

	double sum = 0;
	for (double x : temps)sum += x;
	cout << "Average temperature: " << sum / temps.size() << '\n';


	sort(temps);

	if (temps.size() % 2 != 0)
		cout << "Median temperature: " << temps[temps.size() / 2] << '\n';
	else
		cout << "Median temperature: " << (temps[(temps.size() / 2)-1] + temps[temps.size()/2])/2 << '\n';

	keep_window_open();
	return 0;
}


練習問題 3.
#include "std_lib_facilities.h"

int main()
{
	cout << "距離を入力してください。\n";
	vector<double> roads;
	double road;
	while (cin >> road)
		roads.push_back(road);

	sort(roads);
	double sum=0;
	for (double x : roads)sum += x;

	cout << "total: " << sum << '\n'
		<< "shotest: " << roads[0] << '\n'
		<< "longest: " << roads[roads.size() - 1] << '\n'
		<< "average: " << sum / roads.size() << '\n';

	keep_window_open();
	return 0;
}

練習問題 4.

#include"std_lib_facilities.h"

int main()
{
	vector<int>range(100);

	//range = {1,2,3 .....100}
	for (int i = 1; i < 101; ++i) {
		range[i - 1] = i;
	}

	char ans= ' ';
	int even_or_not = 0;
	cout << " Think one number.\n"
		<<"Please answer my question.(max:7)\n\n";
	cout << "Is the number even?(y or n)\n";
	cin >> ans;
	
	//when it's even
	if (ans == 'y') {
		even_or_not = 1;
		while (range.size() > 99) {
			cout << "Is the number less than " << range[range.size() / 2] << "?\n";//less than {range[50]==51}?
			cin >> ans;
			if (ans == 'y') {//erase (51-100)　resultsize: 50
				range.erase(range.begin() + (range.size() / 2), range.begin() + (range.size()));
			}
			else if (ans == 'n') {//erase (1-50)  resultsize: 50
				range.erase(range.begin(), range.begin() + (range.size() / 2));
			}
		}

		while (range.size() > 49) {
			cout << "Is the number less than " << range[range.size() / 2 - 1] << "?\n";//less than half(25or75)?
			cin >> ans;
			if (ans == 'y') {//erase (half+1 - and above)resultsize: 25
				range.erase(range.begin() + (range.size() / 2), range.begin() + (range.size()));
			}
			else if (ans == 'n') {//erase (1 - half)resultsize: 25
				range.erase(range.begin(), range.begin() + (range.size() / 2));
			}
		}
			while (range.size() > 7) {

				if ((range[range.size() / 2]) % 2 == 0) {//when the number center of vector is even. 
					cout << "Is the number less than " << range[range.size() / 2] + 1 << "?\n";//? A(38, , ,)
					cin >> ans;
					if (ans == 'y') {//erase A+1 - range.size()
						range.erase(range.begin() + (range.size() / 2 + 1), range.begin() + (range.size()));
					}
					else if (ans == 'n') {//erase range[0] -  A-1 
						range.erase(range.begin(), range.begin() + (range.size() / 2 + 1));
					}
					for (int x : range)cout << x << '\n';
				}

				else if ((range[range.size() / 2]) % 2 != 0) {
					cout << "Is the number less than " << range[range.size() / 2]  << "?\n";//? A(13,63,33,83)
					cin >> ans;
					if (ans == 'y') {//erase A+1 - range.size()
						range.erase(range.begin() + (range.size() / 2 + 1), range.begin() + (range.size()));
					}
					else if (ans == 'n') {//erase range[0] -  A-1 
						range.erase(range.begin(), range.begin() + (range.size() / 2 ));
					}

					for (int x : range)cout << x << '\n';
				}
			}
		}


	//odd

	else if (ans == 'n') {
		while (range.size() > 99) {
			cout << "Is the number less than " << range[range.size() / 2 -1] << "?\n";//less than {range[49]==50}?
			cin >> ans;
			if (ans == 'y') {//erase (51-100)　resultsize: 50
				range.erase(range.begin() + (range.size() / 2), range.begin() + (range.size()));
			}
			else if (ans == 'n') {//erase (1-50)  resultsize: 50
				range.erase(range.begin(), range.begin() + (range.size() / 2));
			}
			for (int x : range)cout << x << '\n';
		}

		while (range.size() > 49) {
			cout << "Is the number less than " << range[range.size() / 2 ] << "?\n";//less than half+1 (26or76)?
			cin >> ans;
			if (ans == 'y') {//erase (half+1 - and above)
				range.erase(range.begin() + (range.size() / 2), range.begin() + (range.size()));
			}
			else if (ans == 'n') {//erase (1 - half)
				range.erase(range.begin(), range.begin() + (range.size() / 2));
			}
			for (int x : range)cout << x << '\n';


			while (range.size() > 7) {//
				if ((range[range.size() / 2]) % 2 != 0) {//when the center is even
					cout << "Is the number less than " << range[range.size() / 2] + 1 << "?\n";//? A(13,63,33,83)
					cin >> ans;
					if (ans == 'y') {//erase A+1 - range.size()
						range.erase(range.begin() + (range.size() / 2 + 1), range.begin() + (range.size()));
					}
					else if (ans == 'n') {//erase range[0] -  A-1 
						range.erase(range.begin(), range.begin() + (range.size() / 2 + 1));
					}
					for (int x : range)cout << x << '\n';
				}
				else if ((range[range.size() / 2]) % 2 == 0) {//when the center is odd
					cout << "Is the number less than " << range[range.size() / 2] << "?\n";//? A(38, , ,)
					cin >> ans;
					if (ans == 'y') {//erase A+1 - range.size()
						range.erase(range.begin() + (range.size() / 2 + 1), range.begin() + (range.size()));
					}
					else if (ans == 'n') {//erase range[0] -  A-1 
						range.erase(range.begin(), range.begin() + (range.size() / 2));
					}
					for (int x : range)cout << x << '\n';
				}
			}
		}
	}
	
	while (range.size() > 3)//all way to HERE
	{
	if (even_or_not == 1) {//when the number we want is even

		if (range[0] % 2 == 0) {//when the first element is even
			cout << "Is the number less than " << range[range.size() / 2] << "?\n";//? when(26 27 28 29 30 31 32) remains then 29
			cin >> ans;
			if (ans == 'y') {//erase (center and above)
				range.erase(range.begin() + (range.size() / 2) + 1, range.begin() + range.size());
				for (int x : range)cout << x << '\n';
			}
			else if (ans == 'n') {//erase (1 - center)
				range.erase(range.begin(), range.begin() + (range.size() / 2));
				for (int x : range)cout << x << '\n';
			}
		}

		else if ((range[0] % 2 != 0)) {//when range[0] is odd

			cout << "Is the number less than " << range[range.size() / 2 - 1] << "?\n";//? when(1 2 3 4 5 6 7) remains then what
			cin >> ans;
			if (ans == 'y') {
				cout << "the number you are thinking is " << range[1] << ".\n";
				for (int x : range)cout << x << '\n';
				keep_window_open();
				return 0;
			}
			else if (ans == 'n') {//erase [0] - [2]
				range.erase(range.begin(), range.begin() + (range.size() / 2) );
				for (int x : range)cout << x << '\n';
				cout << "Is the number less than " << range[1] << "?\n";//?w(4 5 6 7) remain then 5
				cin >> ans;
				if (ans == 'y') {
					cout << "the number you are thinking is " << range[0] << ".\n";
					break;
				}
				else if (ans == 'n') {
					cout << "the number you are thinking is " << range[2] << ".\n";
				}
				break;

			}
		}
		
			
		}
	

	else if (even_or_not == 0)//when the number is odd
	{
		if (range[0] % 2 == 0) {//first element is even
			cout << "Is the number less than " << range[range.size() / 2 - 1] << "?\n";//? when(26 27 28 29 30 31 32) remains then 28
			cin >> ans;
			if (ans == 'y') {//
				cout << "the number you are thinking is " << range[1] << ".\n";
				keep_window_open();
				return 0;
			}
			else if (ans == 'n') {//erase 1 - center-1
				range.erase(range.begin(), range.begin() + (range.size() / 2));
				cout << "Is the number less than " << range[1] << "?\n";//?w(4 5 6 7) remain then 5
				cin >> ans;
				if (ans == 'y') {
					cout << "the number you are thinking is " << range[0] << ".\n";
					break;
				}
				else if (ans == 'n') {
					cout << "the number you are thinking is " << range[2] << ".\n";
				}
				break;
			}
		}

		else if ((range[0] % 2 != 0)) {//when range[0] is odd

			cout << "Is the number less than " << range[range.size() / 2 ] << "?\n";//? when(1 2 3 4 5 6 7) remains then what
			cin >> ans;
			if (ans == 'y') {//erase center - range.size()
				range.erase(range.begin() + (range.size() / 2) , range.begin() + range.size());

				cout << "Is the number less than " << range[1] << "?\n";//?w(4 5 6 7) remain then 5
				cin >> ans;
				if (ans == 'y') {
					cout << "the number you are thinking is " << range[0] << ".\n";
					break;
				}
				else if (ans == 'n') {
					cout << "the number you are thinking is " << range[2] << ".\n";
				}
				break;
			}
			else if (ans == 'n') {//erase [0] - [2]
				range.erase(range.begin(), range.begin() + (range.size() / 2));
				
				cout << "Is the number less than " << range[2] << "?\n";//?w(4 5 6 7) remain then 5
				cin >> ans;
				if (ans == 'y') {
					cout << "the number you are thinking is " << range[1] << ".\n";
					break;
				}
				else if (ans == 'n') {
					cout << "the number you are thinking is " << range[3] << ".\n";
				}
				break;
			}
		}
	}
	
	}

	keep_window_open();
	return 0;
}



練習問題 5.

#include "std_lib_facilities.h"

int main()
{
	double val1, val2;
	char oper = ' ';
	cout << "Enter 2 numbers followed by (+,-,*,/)\n";

	while (cin >> val1 >> val2 >> oper) {
		switch (oper) {
		case '+':
			cout << "The sum of " << val1 << " and " << val2 << " is " << val1 + val2 << '\n';
			break;
		case '-':
			cout << "The dif of " << val1 << " minus " << val2 << " is " << val1 - val2 << '\n';
			break;
		case '*':
			cout << "The pro of " << val1 << " mul " << val2 << " is " << val1 * val2 << '\n';
			break;
		case '/':
			cout << "The quo of " << val1 << " div " << val2 << " is " << val1 / val2 << '\n';
			break;
		}
	}
	keep_window_open();
	return 0;
}


練習問題 6-1

#include "std_lib_facilities.h"

int main()
{
	vector<string> number(10);
	int num;
	cout << "Enter one digit(1-9)\n";

		number[0] = "zero";
		number[1] = "one";
		number[2] = "two";
		number[3] = "three";
		number[4] = "four";
		number[5] = "five";
		number[6] = "six";
		number[7] = "seven";
		number[8] = "eight";
		number[9] = "nine";

while (cin >> num) {
		cout << number[num] << '\n';
	}

	keep_window_open();
	return 0;
}

 6-2
 #include "std_lib_facilities.h"

int main()
{
	vector<string> number(10);
	string num = " ";
	cout << "Enter one number(like  one or two or three or ...)\n";

	number[0] = "zero";
	number[1] = "one";
	number[2] = "two";
	number[3] = "three";
	number[4] = "four";
	number[5] = "five";
	number[6] = "six";
	number[7] = "seven";
	number[8] = "eight";
	number[9] = "nine";

	while (cin >> num) {
		int i = -1;
		for (string x : number) {
			++i;
			if (x == num)cout << i << '\n';
		}
	}

	keep_window_open();
	return 0;
}


練習問題 7.

#include "std_lib_facilities.h"

int main()
{
	vector<string> number(20);
	string val1, val2;
	double num1, num2;
	char oper = ' ';

	cout << "Enter two number followed by (+,-,*,/)\n";

	number[0] = "zero";
	number[1] = "one";
	number[2] = "two";
	number[3] = "three";
	number[4] = "four";
	number[5] = "five";
	number[6] = "six";
	number[7] = "seven";
	number[8] = "eight";
	number[9] = "nine";
	number[10] = "0";
	number[11] = "1";
	number[12] = "2";
	number[13] = "3";
	number[14] = "4";
	number[15] = "5";
	number[16] = "6";
	number[17] = "7";
	number[18] = "8";
	number[19] = "9";

	while (cin >> val1 >> val2) {
		for (int i = 0; i < 10; ++i) {
			if (number[i] == val1)
				num1 = i;
			if (number[i] == val2)
				num2 = i;
		}
		for (int i = 10; i < 20; ++i) {
			if (number[i] == val1)
				num1 = i - 10;
			if (number[i] == val2)
				num2 = i - 10;
		}

		while (cin >> oper) {
			switch (oper) {
			case '+':
				cout << "The sum of " << val1 << " and " << val2 << " is " << num1 + num2 << '\n';
				break;
			case '-':
				cout << "The dif of " << val1 << " minus " << val2 << " is " << num1 - num2 << '\n';
				break;
			case '*':
				cout << "The pro of " << val1 << " mul " << val2 << " is " << num1 * num2 << '\n';
				break;
			case '/':
				cout << "The quo of " << val1 << " div " << val2 << " is " << num1 / num2 << '\n';
				break;
			}
			break;
		}
	}
	
	keep_window_open();
	return 0;
}


練習問題 8.

#include "std_lib_facilities.h"

int main()
{
	int reward = 1;
	int sum_of_reward = 0;
	int square_number = 0;

	while(sum_of_reward<1000000001){
		++square_number;
		
		if(square_number >= 2)
			reward *= 2;
		
		sum_of_reward += reward;

		cout << "Square number: " << square_number
			<< "\nReward at this square: " << reward
			<< "\nAll reward: " << sum_of_reward << '\n';
	}	
	keep_window_open();
	return 0;
}

練習問題 9.

#include "std_lib_facilities.h"

int main()
{
	
	double reward = 1;
	double sum_of_reward = 0;
	double square_number = 0;

	while(square_number<64){
		++square_number;
		
		if(square_number >= 2)
			reward *= 2;
		
		sum_of_reward += reward;

		cout << "Square number: " << square_number
			<< "\nReward at this square: " << reward
			<< "\nAll reward: " << sum_of_reward << '\n';
	}
	
	keep_window_open();
	return 0;
}


練習問題 10.

#include "std_lib_facilities.h"

int main()
{
	vector<int>rps;
	int val = 0;
	char your_hand = ' ';
	char my_hand = ' ';
	cout << "please enter some integer";
	while (cin >> val) {
		rps.push_back(val);
	}

	cin.clear();
	cout << "please enter r(for rock)p(for paper)s(for scissors)\n";
	for (int i = 0; i < rps.size(); ++i) {
		if (rps[i] % 5 == 0 && rps[i] % 2 != 0 && rps[i] % 7 != 0) { my_hand = 'r'; }
		else if (rps[i] % 2 == 0 && rps[i] % 7 == 0) { my_hand = 'p'; }
		else { my_hand = 's'; }
		cout << "you: ";
		cin >> your_hand;
		cout << "me: " << my_hand;

	switch (your_hand) {
		case 's':
			if (my_hand == 'p') { cout << "\nYOU WIN!!\n"; break; }
			if (my_hand == 'r') { cout << "\nYOU LOSE.\n"; break; }
			if (my_hand == 's') { cout << "\ndraw\n"; break; }
		case 'r':
			if (my_hand == 's'){ cout << "\nYOU WIN!!\n"; break;}
			if (my_hand == 'p'){ cout << "\nYOU LOSE.\n"; break;}
			if (my_hand == 'r'){ cout << "\ndraw\n"; break;}
		case 'p':
			if (my_hand == 'r') { cout << "\nYOU WIN!!\n"; break; }
			if (my_hand == 's') { cout << "\nYOU LOSE.\n"; break; }
			if (my_hand == 'p') { cout << "\ndraw\n"; break; }
		}
	}
	keep_window_open();
	return 0;
}


練習問題 11.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	

	for (int i = 4 ; i <= 100; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size()-1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}

	cout << "1から100までのうちの素数は\n";
	for (int y : primes) {
		cout << y << "\n";
	}
	cout << "です。\n";

	return 0;
}

練習問題 12.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	int max;
	cout << "1からいくつまでの素数を見つけ出しますか？\n";

	cin >> max;

	for (int i = 4 ; i <= max; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size()-1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}

	cout << "1から"<<max<<"までのうちの素数は\n";
	for (int y : primes) {
		cout << y << "\n";
	}
	cout << "です。\n";

	return 0;
}

練習問題 13.
#include "../../std_lib_facilities.h"

int main()
{
	vector<int> eratos;
	vector<int> primes = { 2 };

	cout << "素数を求めます。\n";
	cout << "いくつまでの素数を求めますか\n今回は百までです。\n";
	
	//limitまでの2の倍数をeratos先に入れる。
	for (int m = 1; 2 * m <= 100; ++m) {
		eratos.push_back(2 * m);

	}

	for (int i = 2; i <= 100; ++i)
	{
		int judge = 0;				//eratos要素にあるかないかを確かめる変数

		for (int y : eratos) {
			if (i == y) { ++judge; }
			else {}
		}


		if (judge == 0) {		//なかった場合にprimesに格納
			primes.push_back(i);
											//考え方が全くだめなやつ
			for (int j = 1; i*j <= 100; ++j) {				//	for (int j = 2; i*j <= 100; ++j) {
				int judge1 = 0;						//
				for (int ii = 0; ii <= eratos.size() - 1; ++ii) {	//	for (int x : eratos) {		//要素にないものをeratosに格納
					if (i*j == eratos[ii]) { ++judge1; }		//		if (i*j == x) {}
					else {}						//		else {
				}							//			eratos.push_back(i*j);
				if (judge1 == 0)					//		}
					eratos.push_back(i*j);				//	}
			}								//}
		}
		else {}		//すでにiがeratosにあった場合
	}
		cout << 100 << "までの素数は\n";
		for (int n : primes) {
			cout << n << '\n';
		}

		return 0;
}

練習問題 14.

#include "../../std_lib_facilities.h"

int main()
{
	int max;
	vector<int> eratos;
	vector<int> primes = { 2 };

	cout << "素数を求めます。\n";
	cout << "いくつまでの素数を求めますか?\n";
	cin >> max;

	//maxまでの2の倍数をeratos先に入れる。
	for (int m = 1; 2 * m <= max; ++m) {
		eratos.push_back(2 * m);

	}

	for (int i = 2; i <= max; ++i)
	{
		int judge = 0;				//eratos要素にあるかないかを確かめる変数

		for (int y : eratos) {
			if (i == y) { ++judge; }
			else {}
		}


		if (judge == 0) {		//なかった場合にprimesに格納
			primes.push_back(i);
																	//考え方が全くだめなやつ
			for (int j = 1; i*j <= max; ++j) {						//	for (int j = 2; i*j <= 100; ++j) {
				int judge1 = 0;										//
				for (int ii = 0; ii <= eratos.size() - 1; ++ii) {	//	for (int x : eratos) {		//要素にないものをeratosに格納
					if (i*j == eratos[ii]) { ++judge1; }			//		if (i*j == x) {}
					else {}											//		else {
				}													//			eratos.push_back(i*j);
				if (judge1 == 0)									//		}
					eratos.push_back(i*j);							//	}
			}														//}
		}
		else {}		//すでにiがeratosにあった場合
	}
		cout << max << "までの素数は\n";
		for (int n : primes) {
			cout << n << '\n';
		}

		return 0;
}

練習問題 15.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	int max;
	int n;
	cout << "あらかじめ、1からいくつの数字までの素数を記憶しますか？\n";
	cin >> max;

	cout << "何個目までの素数を出力しますか？\n";
	cin >> n;

	for (int i = 4; i <= max; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size() - 1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}


	if (n <= primes.size() ) {
		cout << n << "個の素数は\n";
		for (int y = 0; y < n;++y) {
			cout << primes[y] << "\n";
		}
		cout << "です。\n";
	}
	else {
		cout <<n<< "個目の素数の値は" << max << "を超えています。\n" << max << "以下の素数は\n";
		for (int x : primes) {
			cout << x<<'\n';
		}
		cout << "で、"<<primes.size()<<"個です。\n";
	}

	return 0;
}

練習問題 16.		//ああああーーーー疲れたああああああああああ。。。。。。。。
			   //要素がないvectorのnumに値を代入しようとしてたみたいで、それを見落としてかなかなか進まなかった。
			   //これだと100までしかできないけどとりあえず先に進みます。
			   //あ、あと最頻値個数がかぶった時後ろの値しか出せないやコレ。。。。

#include "../../std_lib_facilities.h"

double maxv = 0, miniv = 0, sum = 0;

void otherdenyer(int aa, double val2)
{
	if (aa == 0) {
		maxv = val2;
		miniv = val2;
	}
	else if (aa != 0 && val2 > maxv) {
		maxv = val2;
		cout << "the longest so far\n";
	}
	else if (aa != 0 && val2 < miniv) {
		miniv = val2;
		cout << "the shortest so far\n";
	}
	else {}
}


int main()
{
	int j = 0;
	double val1 = 0;
	string unit;
	vector<double> tsudoi;
	vector<int> num(100);　　　　//ここでvector<int>num;だけだと、74行目でエラーが起きる。push_back()を使う必要があるのかな？？？

	cout << "入力された長さをｍに変換し最大値と最小値を追跡します。単位は(cm,m,in,ft)の4つに限ります。|を入力すると終了します。終了後に、最小値と最大値と値の個数と値の合計を書き出します。\n";

	for (int i = 0; cin >> val1 >> unit; ++i) {

		if (unit == "cm") {
			val1 *= 0.01;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);				//otherdenyerってcmminft以外は入れないってことか。
		}
		else if (unit == "m") {
			{}
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else if (unit == "in") {
			val1 *= 0.0254;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else if (unit == "ft") {
			//1ft == 12in == 12*0.0254m
			constexpr double fttom = 12 * 0.0254;
			val1 *= fttom;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else {
			cout << "Denied.Please enter another length followed by(cm,m,in,ft).\n";
			--i;
		}
		j = i;
	}

	for (int k = 0; k < tsudoi.size()-1; ++k) {
		for (int L = k + 1; L < tsudoi.size(); ++L) {
			if (tsudoi[k] == tsudoi[L]) num[k] += 1;
			else{}
		}
	}
	int exit = 0;
	for (int i = 0; i < num.size()-1; ++i) {
		if (exit == 1)break;
		for (int j = i+1; j < num.size();++j)
			if (num[i] > num[j]) {
				if (j == num.size() - 1) {
					exit = 1;
					cout << "最頻値は" << tsudoi[i] << "です。\n";
				}
			}
		else break;
	}

	cout << "最小値は" << miniv << "m, 最大値は" << maxv << "m, 値の個数は" << j + 1 << ", 値の合計は" << sum << "mです。\n";
	

	return 0;
}

練習問題 17.

#include "../../std_lib_facilities.h"


int main()
{
	int j = 0;
	string val1;
	vector<string> tsudoi;
	vector<int> num(100);
	string maxv = " ", miniv = " ";



	cout << "入力された文字列の(辞書式に並べた時の)最大値と最小値を追跡します。\n";

	for (int i = 0; cin >> val1; ++i) {

			tsudoi.push_back(val1);

				if (i == 0) {
					maxv = val1;
					miniv = val1;
				}
				else if (i != 0 && val1 > maxv) {
					maxv = val1;
					cout << "the latest so far\n";			//is my english right?
				}
				else if (i != 0 && val1 < miniv) {
					miniv = val1;
					cout << "the firstest so far\n";		//correct english?
				}
				else {}

				j = i;
	}

	for (int k = 0; k < tsudoi.size()-1; ++k) {
		for (int L = k + 1; L < tsudoi.size(); ++L) {
			if (tsudoi[k] == tsudoi[L]) num[k] += 1;
			else{}
		}
	}
	int exit = 0;
	for (int i = 0; i < num.size()-1; ++i) {
		if (exit == 1)break;
		for (int j = i+1; j < num.size();++j)
			if (num[i] > num[j]) {
				if (j == num.size() - 1) {
					exit = 1;
					cout << "最頻値は" << tsudoi[i] << "です。\n";
				}
			}
		else break;
	}

	cout << "最小値は" << miniv << ", 最大値は" << maxv << ", 値の個数は" << j + 1 << "です。\n";
	

	return 0;
}

練習問題 18.

#include "../../std_lib_facilities.h"

int main()
{
	double a, b, c;
	double ans1, ans2;
	double bb4ac;
	double rootbb4ac;
	cout << "ax^2+bx+cの形の2次方程式を解きます。\na、b、cの順に入力してください。\n";
	cin >> a >> b >> c;

	if (b*b - 4 * a*c >= 0) {
		bb4ac = b*b - 4 * a*c;
		rootbb4ac = sqrt(bb4ac);
		ans1 = -1 * (b - rootbb4ac) / 2 * a;
		ans2 = -1 * (b + rootbb4ac) / 2 * a;
		cout << "この2次方程式の解は、" << ans1 << "と" << ans2 << "です。\n";
	}
	else cout << "この2次方程式の解は実数の範囲にありません。\n";

	return 0;
}

こっちの方が繰り返せていいかな？　18.

#include "../../std_lib_facilities.h"

int main()
{
	double a, b, c;
	double ans1, ans2;
	double bb4ac;
	double rootbb4ac;
	cout << "ax^2+bx+cの形の2次方程式を解きます。\na、b、cの順に入力してください。\n";
	for (; cin >> a >> b >> c;){

		if (b*b - 4 * a*c >= 0) {
			bb4ac = b*b - 4 * a*c;
			rootbb4ac = sqrt(bb4ac);
			ans1 = -1 * (b - rootbb4ac) / 2 * a;
			ans2 = -1 * (b + rootbb4ac) / 2 * a;
			cout << "この2次方程式の解は、" << ans1 << "と" << ans2 << "です。\n";
		}
		else cout << "この2次方程式の解は実数の範囲にありません。\n";
}
	return 0;
}


//練習問題 19.
//これだと abort() has been calledになる。なぜだあああああああ
#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score;++i) {
		if (i == 0) {
			names[0] = name;
			scores[0] = score;
		}
		else {
			int judge = 0;
			for (int j = 1; j <= i; ++j) {
				if (names[i] == names[j]) {			//ここがダメらしい。
					judge += 1;
					}
			}
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
			}

			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}
	for (int i = 0; i < names.size();++i) {
		cout << names[i] << " " << scores[i] << '\n';
	}
	return 0;
}

練習問題 19.		//これならok!!なぜだかはわからない。

#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score; ++i) {
		int judge = 0;
		if (i == 0) {
			names[i] = name;
			scores[i] = score;
		}
		else {
			for (int j = 0; j <= i; ++j) {
				if(i == j){}
				else if (name == names[j]) {			name[i]==name[j]だとだめなのかな？？？？
					judge += 1;
					}
				else{}
			}
			
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
				name = " ";
				score = 0;
				--i;						//ここに--i;がないと、abort() has been calledになる。
			}
			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}
	for (int i = 0; i < names.size();++i) {
		cout << names[i] << " " << scores[i] << '\n';
	}

	return 0;
}

練習問題 20.		//前にもあったんだけど、最初のfor文をintに文字リテラル入れて終了させると自動的に、後ろのfor文も終了しちゃう。

#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;
	string thename = " ";

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score; ++i) {
		int judge = 0;
		if (i == 0) {
			names[i] = name;
			scores[i] = score;
		}
		else {
			for (int j = 0; j <= i; ++j) {
				if(i == j){}
				else if (name == names[j]) {
					judge += 1;
					}
				else{}
			}
			
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
				name = " ";
				score = 0;
				--i;
			}
			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}


	cin.clear();								//これでいいのかな？
	cin.ignore();								//調べたら見つかったんだけど。
	cout << "入力された中から、指定された名前の方の点数を出力します。\n";
	
	for (int y = 0; cin >> thename; ++y) {
		int judge = 0;
		for (int x = 0; x < names.size(); ++x) {
			if (thename == names[x]) {
				cout << thename << "さんの点数は" << scores[x] << "です。\n";
				++judge;
			}
		}
		if (judge == 0) {
			cout << "name not found.\n";
		}
	}

	return 0;
}

練習問題 21.

#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;
	int thescore = 0;

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score; ++i) {
		int judge = 0;
		if (i == 0) {
			names[i] = name;
			scores[i] = score;
		}
		else {
			for (int j = 0; j <= i; ++j) {
				if(i == j){}
				else if (name == names[j]) {
					judge += 1;
					}
				else{}
			}
			
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
				name = " ";
				score = 0;
				--i;
			}
			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}
	cin.clear();
	cin.ignore();
	cout << "先に入力された中から、指定された点数の方の名前を出力します。\n";
	
	for (int y = 0; cin >> thescore; ++y) {
		int judge = 0;
		for (int x = 0; x < names.size(); ++x) {
			if (thescore == scores[x]) {
				cout <<"点数が"<< thescore << "の方は" << names[x] << "さんです。\n";
				++judge;
			}
		}
		if (judge == 0) {
			cout << "score not found.\n";
		}
	}

	return 0;
}
