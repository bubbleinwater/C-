練習問題 10.			//uuummよくわからんなあああーー。
#include "../../std_lib_facilities.h"

int main()
{
	vector<char> jankens;
	char janken;
	char te = ' ';
	cout << "まず初めに、それぞれ3つずつ、文字、（p,s,r）を好きな順番で入力してください。\n";

	for (int j = 0; j < 9;++j) {
		cin >> janken;
		jankens.push_back(janken);
	}
	if (jankens[3] == 'p') {
		char tmp = jankens[4];
		jankens[4] = 's';
		jankens[6] = 's';
		jankens[2] = tmp;
	}
	if (jankens[7] == 'r') {
		char tmp = jankens[1];
		jankens[1] = 'r';
		jankens[2] = 'p';
		jankens[3] = 's';
		jankens[4] = tmp;
	}
	if (jankens[5] == 's') {
		jankens[1] = 's';
		jankens[3] = 'r';
		jankens[8] = 'r';
	}

	cout << "それでは、じゃんけんをしましょう。\nあなたの入力を合図にランダムに(p,s,r)のどれかを出力します。\n";
	for (int i = 0; i < 8; ++i) {
	
		cout << "\nじゃんけん\n";
		
		cin >> te;
		cout << "ぽいっ" << jankens[i];
	}
		return 0;
}

練習問題 11.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	

	for (int i = 4 ; i <= 100; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size()-1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}

	cout << "1から100までのうちの素数は\n";
	for (int y : primes) {
		cout << y << "\n";
	}
	cout << "です。\n";

	return 0;
}

練習問題 12.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	int max;
	cout << "1からいくつまでの素数を見つけ出しますか？\n";

	cin >> max;

	for (int i = 4 ; i <= max; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size()-1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}

	cout << "1から"<<max<<"までのうちの素数は\n";
	for (int y : primes) {
		cout << y << "\n";
	}
	cout << "です。\n";

	return 0;
}

練習問題 13.
#include "../../std_lib_facilities.h"

int main()
{
	vector<int> eratos;
	vector<int> primes = { 2 };

	cout << "素数を求めます。\n";
	cout << "いくつまでの素数を求めますか\n今回は百までです。\n";
	
	//limitまでの2の倍数をeratos先に入れる。
	for (int m = 1; 2 * m <= 100; ++m) {
		eratos.push_back(2 * m);

	}

	for (int i = 2; i <= 100; ++i)
	{
		int judge = 0;				//eratos要素にあるかないかを確かめる変数

		for (int y : eratos) {
			if (i == y) { ++judge; }
			else {}
		}


		if (judge == 0) {		//なかった場合にprimesに格納
			primes.push_back(i);
											//考え方が全くだめなやつ
			for (int j = 1; i*j <= 100; ++j) {				//	for (int j = 2; i*j <= 100; ++j) {
				int judge1 = 0;						//
				for (int ii = 0; ii <= eratos.size() - 1; ++ii) {	//	for (int x : eratos) {		//要素にないものをeratosに格納
					if (i*j == eratos[ii]) { ++judge1; }		//		if (i*j == x) {}
					else {}						//		else {
				}							//			eratos.push_back(i*j);
				if (judge1 == 0)					//		}
					eratos.push_back(i*j);				//	}
			}								//}
		}
		else {}		//すでにiがeratosにあった場合
	}
		cout << 100 << "までの素数は\n";
		for (int n : primes) {
			cout << n << '\n';
		}

		return 0;
}

練習問題 14.

#include "../../std_lib_facilities.h"

int main()
{
	int max;
	vector<int> eratos;
	vector<int> primes = { 2 };

	cout << "素数を求めます。\n";
	cout << "いくつまでの素数を求めますか?\n";
	cin >> max;

	//maxまでの2の倍数をeratos先に入れる。
	for (int m = 1; 2 * m <= max; ++m) {
		eratos.push_back(2 * m);

	}

	for (int i = 2; i <= max; ++i)
	{
		int judge = 0;				//eratos要素にあるかないかを確かめる変数

		for (int y : eratos) {
			if (i == y) { ++judge; }
			else {}
		}


		if (judge == 0) {		//なかった場合にprimesに格納
			primes.push_back(i);
																	//考え方が全くだめなやつ
			for (int j = 1; i*j <= max; ++j) {						//	for (int j = 2; i*j <= 100; ++j) {
				int judge1 = 0;										//
				for (int ii = 0; ii <= eratos.size() - 1; ++ii) {	//	for (int x : eratos) {		//要素にないものをeratosに格納
					if (i*j == eratos[ii]) { ++judge1; }			//		if (i*j == x) {}
					else {}											//		else {
				}													//			eratos.push_back(i*j);
				if (judge1 == 0)									//		}
					eratos.push_back(i*j);							//	}
			}														//}
		}
		else {}		//すでにiがeratosにあった場合
	}
		cout << max << "までの素数は\n";
		for (int n : primes) {
			cout << n << '\n';
		}

		return 0;
}

練習問題 15.

#include "../../std_lib_facilities.h"

int main()
{
	vector<int> primes = { 2,3 };
	int rem;
	int max;
	int n;
	cout << "あらかじめ、1からいくつの数字までの素数を記憶しますか？\n";
	cin >> max;

	cout << "何個目までの素数を出力しますか？\n";
	cin >> n;

	for (int i = 4; i <= max; i++) {
		int judge = 0;
		for (int j = 0; j < primes.size() - 1; ++j) {
			rem = i % primes[j];
			if (rem == 0) { ++judge; }
			else {}
		}
		if (judge == 0) {
			primes.push_back(i);
		}
	}


	if (n <= primes.size() ) {
		cout << n << "個の素数は\n";
		for (int y = 0; y < n;++y) {
			cout << primes[y] << "\n";
		}
		cout << "です。\n";
	}
	else {
		cout <<n<< "個目の素数の値は" << max << "を超えています。\n" << max << "以下の素数は\n";
		for (int x : primes) {
			cout << x<<'\n';
		}
		cout << "で、"<<primes.size()<<"個です。\n";
	}

	return 0;
}

練習問題 16.		//ああああーーーー疲れたああああああああああ。。。。。。。。
			   //要素がないvectorのnumに値を代入しようとしてたみたいで、それを見落としてかなかなか進まなかった。
			   //これだと100までしかできないけどとりあえず先に進みます。
			   //あ、あと最頻値個数がかぶった時後ろの値しか出せないやコレ。。。。

#include "../../std_lib_facilities.h"

double maxv = 0, miniv = 0, sum = 0;

void otherdenyer(int aa, double val2)
{
	if (aa == 0) {
		maxv = val2;
		miniv = val2;
	}
	else if (aa != 0 && val2 > maxv) {
		maxv = val2;
		cout << "the longest so far\n";
	}
	else if (aa != 0 && val2 < miniv) {
		miniv = val2;
		cout << "the shortest so far\n";
	}
	else {}
}


int main()
{
	int j = 0;
	double val1 = 0;
	string unit;
	vector<double> tsudoi;
	vector<int> num(100);　　　　//ここでvector<int>num;だけだと、74行目でエラーが起きる。push_back()を使う必要があるのかな？？？

	cout << "入力された長さをｍに変換し最大値と最小値を追跡します。単位は(cm,m,in,ft)の4つに限ります。|を入力すると終了します。終了後に、最小値と最大値と値の個数と値の合計を書き出します。\n";

	for (int i = 0; cin >> val1 >> unit; ++i) {

		if (unit == "cm") {
			val1 *= 0.01;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);				//otherdenyerってcmminft以外は入れないってことか。
		}
		else if (unit == "m") {
			{}
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else if (unit == "in") {
			val1 *= 0.0254;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else if (unit == "ft") {
			//1ft == 12in == 12*0.0254m
			constexpr double fttom = 12 * 0.0254;
			val1 *= fttom;
			tsudoi.push_back(val1);
			sum += val1;
			unit = " ";
			otherdenyer(i, val1);
		}
		else {
			cout << "Denied.Please enter another length followed by(cm,m,in,ft).\n";
			--i;
		}
		j = i;
	}

	for (int k = 0; k < tsudoi.size()-1; ++k) {
		for (int L = k + 1; L < tsudoi.size(); ++L) {
			if (tsudoi[k] == tsudoi[L]) num[k] += 1;
			else{}
		}
	}
	int exit = 0;
	for (int i = 0; i < num.size()-1; ++i) {
		if (exit == 1)break;
		for (int j = i+1; j < num.size();++j)
			if (num[i] > num[j]) {
				if (j == num.size() - 1) {
					exit = 1;
					cout << "最頻値は" << tsudoi[i] << "です。\n";
				}
			}
		else break;
	}

	cout << "最小値は" << miniv << "m, 最大値は" << maxv << "m, 値の個数は" << j + 1 << ", 値の合計は" << sum << "mです。\n";
	

	return 0;
}

練習問題 17.

#include "../../std_lib_facilities.h"


int main()
{
	int j = 0;
	string val1;
	vector<string> tsudoi;
	vector<int> num(100);
	string maxv = " ", miniv = " ";



	cout << "入力された文字列の(辞書式に並べた時の)最大値と最小値を追跡します。\n";

	for (int i = 0; cin >> val1; ++i) {

			tsudoi.push_back(val1);

				if (i == 0) {
					maxv = val1;
					miniv = val1;
				}
				else if (i != 0 && val1 > maxv) {
					maxv = val1;
					cout << "the latest so far\n";			//is my english right?
				}
				else if (i != 0 && val1 < miniv) {
					miniv = val1;
					cout << "the firstest so far\n";		//correct english?
				}
				else {}

				j = i;
	}

	for (int k = 0; k < tsudoi.size()-1; ++k) {
		for (int L = k + 1; L < tsudoi.size(); ++L) {
			if (tsudoi[k] == tsudoi[L]) num[k] += 1;
			else{}
		}
	}
	int exit = 0;
	for (int i = 0; i < num.size()-1; ++i) {
		if (exit == 1)break;
		for (int j = i+1; j < num.size();++j)
			if (num[i] > num[j]) {
				if (j == num.size() - 1) {
					exit = 1;
					cout << "最頻値は" << tsudoi[i] << "です。\n";
				}
			}
		else break;
	}

	cout << "最小値は" << miniv << ", 最大値は" << maxv << ", 値の個数は" << j + 1 << "です。\n";
	

	return 0;
}

練習問題 18.

#include "../../std_lib_facilities.h"

int main()
{
	double a, b, c;
	double ans1, ans2;
	double bb4ac;
	double rootbb4ac;
	cout << "ax^2+bx+cの形の2次方程式を解きます。\na、b、cの順に入力してください。\n";
	cin >> a >> b >> c;

	if (b*b - 4 * a*c >= 0) {
		bb4ac = b*b - 4 * a*c;
		rootbb4ac = sqrt(bb4ac);
		ans1 = -1 * (b - rootbb4ac) / 2 * a;
		ans2 = -1 * (b + rootbb4ac) / 2 * a;
		cout << "この2次方程式の解は、" << ans1 << "と" << ans2 << "です。\n";
	}
	else cout << "この2次方程式の解は実数の範囲にありません。\n";

	return 0;
}
